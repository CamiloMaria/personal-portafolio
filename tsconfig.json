{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@sections/*": ["src/sections/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@cv-es": ["./cv-es.json"],
      "@cv-en": ["./cv-en.json"],
      "@icons/*": ["src/icons/*"]
    }
  }
}
